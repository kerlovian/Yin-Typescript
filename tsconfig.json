{
  "compilerOptions": {
    
    /* Basic Options */
    "target": "es2018",
    "module": "commonjs",
    "removeComments": false,
    
    /* Strict Type-Checking Options */
    "strict": true,
    
    /* Module Resolution Options */
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
